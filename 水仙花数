<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title></title>
	</head>
	<body>
		<p id="p"></p>
		<script>
			for(var i=100;i<=999;i++){
				var a=parseInt(i/100);
				var b=parseInt(i%100/10);
				var c=i%10;
				 if(a*a*a+b*b*b+c*c*c===i){
					document.getElementById("p").innerHTML +=i+"&nbsp";
				}								
			}			
		</script>
	</body>
</html>
